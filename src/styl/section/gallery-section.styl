.gallery-section {

}

.gallery {
  display: flex;
  flex-wrap: wrap
  justify-content center
  margin: -13px -9px

  &__item {
    display: inline-block
    margin: 13px 9px 13px 9px
    max-width 466px
    width: 100%
    height 277px
    object-fit cover
    border-radius 5px
  }
}

@media (max-width: 560px) {
  .gallery {
    margin: -13px 0
    padding: 0 16px
    &__item {
      margin: 8px 9px 8px 9px
    }
  }
}
